{
  "name": "vue-template",
  "private": true,
  "version": "0.0.1",
  "description": "基于Vue+Parcel+Typescript的项目模板",
  "scripts": {
    "serve": "cross-env NODE_OPTIONS=--max-old-space-size=16384 parcel serve ./public/index.html --log-level=info",
    "test": "echo \"Error: no test specified\" && exit 1",
    "browsers": "npx browserslist",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 parcel build ./public/index.html --no-cache --no-source-maps --log-level=error --dist-dir=./build"
  },
  "targets": {
    "main": false
  },
  "author": "0x23fd5097@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.0.1",
    "axios": "^1.3.5",
    "cookies-js": "^1.2.3",
    "dayjs": "^1.11.7",
    "events": "^3.3.0",
    "idb": "^7.1.1",
    "md5": "^2.3.0",
    "os-browserify": "^0.3.0",
    "pinia": "^2.0.34",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/md5": "^2.3.2",
    "@types/node": "^18.15.11"
  },
  "optionalDependencies": {
    "@tsconfig/vite-react": "latest",
    "core-js": "latest",
    "cross-env": "latest",
    "parcel": "latest",
    "typescript": "~5"
  }
